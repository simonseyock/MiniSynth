{
  "name": "MobileSynthMiniSeq",
  "version": "0.0.1",
  "description": "A small sequencer and synthesizer for a mobile.",
  "author": {
    "name": "Simon Seyock",
    "email": "simonseyock@gmail.com"
  },
  "private": true,
  "dependencies": {
    "jquery": "1.11.1",
    "lodash": "3.2.0"
  },
  "devDependencies": {
    "grunt": "^0.4.5",
    "grunt-contrib-jshint": "~0.10.0",
    "grunt-contrib-less": "^1.0.0",
    "grunt-contrib-uglify": "~0.5.0",
    "jshint-stylish": "^1.0.1",
    "uprocess": "latest",
	"less": "latest",
	"parallelshell": "latest",
	"watch": "latest",
	"live-reload": "latest"
  },
  "scripts": {
	"build:js": "grunt uprocess",
	"watch:js": "watch \"npm run build:js\" javascript/",
	"build:css": "lessc less/sequencer-chessboard.less build/css/MobileSynthMiniSeq.css",
	"watch:css": "watch \"npm run build:css\" less/",
	"build": "npm run build:js && npm run build:css",
	"watch": "parallelshell \"npm run watch:js\" \"npm run watch:css\"",
	"livereload": "live-reload --port 9091 build/",
	"serve": "parallelshell \"npm run watch\" \"npm run livereload\""
  }
}
